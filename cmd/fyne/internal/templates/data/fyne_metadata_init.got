package main

import (
	"github.com/nemesis567/fyne"
	"github.com/nemesis567/fyne/app"
)

func init() {
	app.SetMetadata(fyne.AppMetadata{
		ID: "{{.AppID}}",
		Name: "{{.Name}}",
		Version: "{{.AppVersion}}",
		Build: {{.AppBuild}},
		Icon: {{.ResGoString}},
		{{if .VersionAtLeast2_3}}
		Release: {{.Release}},
		Custom: map[string]string{
			{{range $key, $value := .CustomMetadata}}
			"{{$key}}": "{{$value}}",
			{{end}}
		},
		{{end}}
	})
}

